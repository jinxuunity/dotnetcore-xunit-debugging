{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (Tests)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}/bin/Debug/netcoreapp1.0/dotnetcore-xunit-debugging.dll",
            "args": [],
            "cwd": "${workspaceRoot}",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": 0
        }
    ]
}